#variable declaration
var x = 1

#function declaration
fun sum_integers: first_integer, last_integer
   var i = first_integer
  var sum = 0
  while i <= last_integer
    sum = sum + i

    i = i + 1
  end

  println(sum)
end

#function call
sum_integers(1, 100)

